   Name = "Void.lua ┃ MM2",
   LoadingTitle = "Void.lua ┃ MM2",
   LoadingSubtitle = "By xx1xx1x1x1",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = Voidlua, 
      FileName = "Voidd"
   },
   Discord = {
      Enabled = nil,
      Invite = "8eKdgqWuwr", 
      RememberJoins = true 
   },
   KeySystem = false, 
   KeySettings = {
      Title = "Void.lua",
      Subtitle = "Key System",
      Note = "Join the discord (https://discord.gg/8eKdgqWuwr) Last Updated 24/08/24",
      FileName = "VoidKey",
      SaveKey = true,
      GrabKeyFromSite = true, 
      Key = "ZorIs1"
   }
